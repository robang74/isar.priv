# Load common stuff
. ${S}/debian/isar/common

do_clean() {

    # Trace what we do here
    printenv | grep -q BB_VERBOSE_LOGS && set -x

    rm -rf ${deb_img_dir} ${deb_dbg_dir}
    rm -rf ${deb_kern_hdrdir}
    rm -rf ${deb_libc_hdr_dir}

    # Stop tracing
    set +x
}

main clean ${*}
